# Foxglove Agent Configuration
# Copy this file to .env and fill in your values

# Required: Get your device token from https://app.foxglove.dev/settings/devices
FOXGLOVE_DEVICE_TOKEN=your_device_token_here

# Optional: Name for this device in Foxglove
DEVICE_NAME=ros1-recorder

# Optional: Log level for Foxglove Agent (error, warn, info, debug, trace)
# Default: debug
RUST_LOG=trace
